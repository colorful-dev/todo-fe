{
  "buildCommand": "pnpm run build",
  "cleanUrls": true,
  "redirects": [
    { "source": "/*", "destination": "/index.html", "statusCode": 200 }
  ],
  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ]
}
